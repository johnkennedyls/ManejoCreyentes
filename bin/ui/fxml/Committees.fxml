      <Button layoutX="270.0" layoutY="13.0" mnemonicParsing="false" onAction="#loadCommittee" prefHeight="32.0" prefWidth="72.0" style="-fx-background-color: ROYALBLUE;" text="Cargar" textFill="WHITE" />
   </children>
</AnchorPane>
